{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "geo-client",
  "version": "0.1.0",
  "identifier": "com.geo.client",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "端界GEO",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [],
    "resources": [],
    "copyright": "",
    "category": "Utility",
    "shortDescription": "端界GEO客户端",
    "longDescription": "端界GEO营销客户端应用程序"
  },
  "plugins": {}
}
